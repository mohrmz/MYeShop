                    <ul>
                        <li class="hidden"><img class="hidden" itemprop="image" src="~/images/joor.jpg" alt="joorkadeh" title="جور کده" width="20" height="20" /></li>
                        <li><a asp-action="US" asp-controller="Other" title="تماس ب ما"><i class="fa fa-globe"></i><span itemprop="name">تماس با ما جورکده</span> </a></li>
                        <li><a href="tel:+989189660086" title="تلفن همراه"><i class="fa fa-phone"></i><span itemprop="telephone">09189660086</span> </a></li>
                        <li><a href="tel:+982537185340" title="تلفن شرکت "><i class="fa fa-phone"></i><span itemprop="telephone">02537185340</span> </a></li>
                        <li><a href="mailto:joorkadeh@gmail.com" title="ایمیل"><i class="fa fa-envelope"></i><span itemprop="email">joorkadeh@gmail.com</span> </a></li>
                        <li itemprop="address" itemscope itemtype="http://schema.org/PostalAddress" class="font-y">آدرس :<span itemprop="addressRegion">قم </span><span itemprop="addressLocality">خیابان امام کوچه </span><span itemprop="streetAddress">عابدی 12  بن بست بهرامی پلاک 8.1 طبقه زیر زمین</span></li>
                        <li><span class="text-hide" itemprop="priceRange">$$</span></li>
                    </ul>
                </div>

                <div class="clear"></div>
            </div>
        </div><!--/header_top-->





        @await Component.InvokeAsync("Menu")
    </header>


    @RenderBody()

    <div class="hreview" id="hreview-best">
        <h2 class="summary">@ViewData["ShorDescription"]</h2>
        <abbr class="dtreviewed" title="@DateTime.Today">@DateTime.Today</abbr>
        <span class="reviewer vcard">
            <span class="fn"></span>
        </span>
        <span class="type" style="display: none;">@ViewData["Title"]</span>
        <img alt="@ViewData["Title"]" src="@ViewData["image"]" class="photo">
        <div class="item">
            <a class="fn url" href="https://@ViewData["url"]" title="@ViewData["Title"]">@ViewData["Title"]</a>
        </div>
        <blockquote class="description">
            <p>
                <abbr title="5" class="rating">
                    @ViewData["Title"]
                </abbr>
            </p>
        </blockquote>
        <span class="version" style="display: none;">0.3</span>
        <p> <a href="http://microformats.org/wiki/hreview" title="@ViewData["Title"]">@ViewData["Title"]</a> جورکده <a href="http://microformats.org/code/hreview/creator">@ViewData["Title"]</a></p>
    </div>

    <footer id="footer">


