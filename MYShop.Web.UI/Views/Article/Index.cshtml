
@{
    ViewData["Title"] = "فروشگاه اینترنتی جور کده حراج محصولات|مقالات جورکده";
    ViewData["ShorDescription"] = "فروشگاه اینترنتی جور کده فروش بهترین محصولات و برند ها با قیمت مناسب و ارسال رایگان در سرتاسر کشور همراه با گارانتی اصل شرکتی";
    ViewData["KeyWords"] = "جور کده،فروشگاه اینترنتی جورکده،فروش تشک،تشک رویا،تشک،خرید تشک،خرید تشک رویا،خرید محصول";

    ViewData["url"] = Context.Request.Host + Context.Request.GetEncodedPathAndQuery();
    ViewData["image"] = "https://joorkadeh.com/images/joor.jpg";
}

<h1 class="center-align box" >مقالات</h1>

<div class="container-fluid">
    <div class="row">
        <div class="col-md-6">
            <article itemscope itemtype="http://schema.org/Article" class="card text-center " style="max-width: 400px;margin:20px;">
                <div title="انتخاب تشک">
                    <!-- Heading -->
                    <div class="card-body">

                        <h4 itemprop="author" class="card-title text-black-50">راهنمای خرید تشک</h4>
                        <h6 itemprop="headline name" class="card-subtitle text-muted">مهمترین معیار های انتخاب یک تشک مناسب چیست؟</h6>
                        <p itemprop="publisher" itemtype="http://schema.org/Organization" itemscope="">
                            <span itemprop="logo" itemscope itemtype="http://schema.org/ImageObject">
                                <link itemprop="url" href="~/images/joor.jpg" />
                            </span>
                            <span itemprop="name">جورکده</span>
                        </p>

                    </div>

                    <!-- Image -->

                    <meta itemprop="dateModified" content="@DateTime.Today.ToString("yyyy-MM-dd")"> @DateTime.Today.ToString("yyyy-MM-dd")

                    <meta itemprop="datePublished" content="2019-10-12">تاریخ ایجاد : 1398/07/20

                    <img itemprop="image" src="~/images/toshak.jpg" alt="تشک" title="تشک" width="200" class="img-responsive-s" style="margin:auto">

                    <!-- Text Content -->
                    <div class="card-body">
                        <a itemprop="mainEntityOfPage" asp-controller="Article" asp-action="toshak" title="نکات طلایی انتخاب تشک خوب"><p itemprop="name" class="card-text text-black-50">جامع‌ترین راهنمای خرید تشک و نکات طلایی یک انتخاب خوب</p></a>

                        <a asp-controller="Article" asp-action="toshak" title="اصول انتخاب تشک" class="btn-c btn-p">ادامه</a>
                    </div>
                    <div itemprop="interactionStatistic" itemscope itemtype="http://schema.org/InteractionCounter">
                        <div itemprop="interactionService" itemscope itemid="http://www.twitter.com" itemtype="http://schema.org/WebSite">
                            <meta itemprop="name" content="Twitter" />
                        </div>
                        <meta itemprop="interactionType" content="http://schema.org/ShareAction" />
                        <meta itemprop="userInteractionCount" content="1203" />
                    </div>
                    <div itemprop="interactionStatistic" itemscope itemtype="http://schema.org/InteractionCounter">
                        <meta itemprop="interactionType" content="http://schema.org/CommentAction" />
                        <meta itemprop="userInteractionCount" content="78" />
                    </div>
                </div>
